; BIND reverse data file for empty rfc1918 zone
;
; DO NOT EDIT THIS FILE - it is used for multiple zones.
; Instead, copy it, edit named.conf, and use that copy.
;
$TTL	86400
@	IN	SOA	localhost. root.localhost. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			  86400 )	; Negative Cache TTL
;
@		IN	NS	ns.group1.lab.
gw		IN	A	192.168.1.1
ns		IN	A	192.168.1.2
dhcpd		CNAME		ns

client1		IN	A	192.168.1.11
client2		IN	A	192.168.1.12
client3		IN	A	192.168.1.13
client4		IN	A	192.168.1.14
client5		IN	A	192.168.1.15
client6		IN	A	192.168.1.16
client7		IN	A	192.168.1.17
client8		IN	A	192.168.1.18
client9		IN	A	192.168.1.19
client10	IN	A	192.168.1.20

