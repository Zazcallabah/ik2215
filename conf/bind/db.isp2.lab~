; BIND reverse data file for empty rfc1918 zone
;
; DO NOT EDIT THIS FILE - it is used for multiple zones.
; Instead, copy it, edit named.conf, and use that copy.
;
$TTL	86400
@	IN	SOA	ns.isp2.lab. postmaster.isp2.lab. (
			      6		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			  86400 )	; Negative Cache TTL
;

;
; Nameservers
;

isp2.lab.		IN       NS       ns.isp2.lab.
;
; Mailserver
;
mail            IN      A        10.2.8.23
smtp            IN      CNAME    isp2.lab.
imap            IN      CNAME    isp2.lab.
isp2.lab.	IN      MX   10  mail.isp2.lab.
;
; Nodes in domain
;
ns.isp2.lab.			IN      A        10.2.8.22
isp.lab.			IN      A        10.2.8.23
www                             IN      A        10.2.8.20
dhcp                            IN      A        10.2.8.21
vpn                             IN      A        10.2.8.23
www                             IN      A        10.2.8.20

;;Clients

client1				IN	A	 10.2.0.

;Routers

r2611xm.isp2.lab.		IN	A	 10.0.16.6
r2514.isp2.lab.			IN	A	 10.2.8.9
r2501-a.isp2.lab.		IN	A	 10.2.8.6
r2501-b.isp2.lab.		IN	A	 10.2.8.10

;Reverse

;;Routers
6.16.0.10.in-addr.arpa.		IN      PTR      r2611xm.isp2.lab.
9.8.2.10.in-addr.arpa.		IN      PTR      r2514.isp2.lab.
6.8.2.10.in-addr.arpa.		IN      PTR      r2501-a.isp2.lab.
10.8.2.10.in-addr.arpa.		IN      PTR      r2501-b.isp2.lab.

;;Services
20.8.2.10.in-addr.arpa.		IN      PTR      isp2.lab.
22.8.2.10.in-addr.arpa.		IN      PTR      dhcp.isp2.lab.
23.8.2.10.in-addr.arpa.		IN      PTR      vpn.isp2.lab.
